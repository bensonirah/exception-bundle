{
    "name": "tounaf/exception-bundle",
    "description": "Exception handler for symfony project",
    "type": "symfony-bundle",
    "require": {
        "symfony/dependency-injection": "6.2.* || 6.1.* || 5.4.* || 4.4.*",
        "symfony/config": "6.2.* || 6.1.* || 5.4.* || 4.4.*",
        "symfony/framework-bundle": "6.2.* || 6.1.* || 5.4.* || 4.4.*",
        "symfony/http-kernel": "6.2.* || 6.1.* || 5.4.* || 4.4.*"
    },
    "autoload": {
        "psr-4": {
            "Pulse\\ExceptionBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pulse\\ExceptionBundle\\Tests\\": "Tests/"
        }
    },
    "authors": [
        {
            "name": "Fetra Harinjatovo",
            "email": "harinjatovo.fetra@gmail.com"
        }
    ],
    "require-dev": {
        "symfony/test-pack": "^1.1",
        "symfony/var-dumper": "^6.2",
        "squizlabs/php_codesniffer": "*"
    }
}
